@model List<HouseModels>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@foreach (var item in Model)
{
    <div class="col-md-4 col-sm-12 w3_ban1">
        <div class="card" style="background-color:aliceblue">
            <img class="card-img-top" style="height:200px;" src="@item.ImageThumbnail" alt="Card image cap">
            <div class="card-body">
                <h6 class="card-text" style="font-size:18px;">@item.HouseModelName</h6>
                <br />
                <a asp-action="ViewDetails" asp-controller="HouseModelCus" asp-route-id="@item.Id" class="btn btn-outline-primary">Xem chi tiết</a>
            </div>
        </div>
    </div>
}
