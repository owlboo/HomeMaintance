@model List<HomeMaintance.Models.Services>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

@foreach (var item in Model)
{
    <div class="col-md-4 col-sm-12 w3_ban1">
        <div class="card">
            <img class="card-img-top" style="height:200px;" src="~/images/ServiceImages/@item.Thumbnail" alt="Card image cap">
            <div class="card-body">
                <h6 class="card-text" >@item.ServiceName</h6>
                <a asp-action="Index" asp-controller="ServiceDetails" asp-route-id="@item.Id" class="btn btn-primary">Explore</a>
            </div>
        </div>
    </div>
}
